// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Champion{
  id        Int @default(autoincrement()) @id
  title     String
  content   String?
  published Boolean @default(false)

}